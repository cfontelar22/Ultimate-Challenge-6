<h1>Customers Alphabetized by Name</h1>

<div class="customer-container">
  <% if @customers.any? %>
    <%= render partial: 'customer', collection: @customers %>
  <% else %>
    <p>No customers found.</p>
  <% end %>
</div>

<%= link_to 'All Customers', customers_path %> |
<%= link_to 'Missing Email', missing_email_customers_path %> |
